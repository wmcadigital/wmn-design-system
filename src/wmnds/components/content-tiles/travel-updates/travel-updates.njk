   {%- from "wmnds/components/disruption-indicator/medium/_medium.njk" import wmndsDisruptionIndicatorMedium as medium -%}
   {%- from "wmnds/components/disruption-indicator/medium/_medium.njk" import wmndsDisruptionIndicatorMedium as star -%}
   <div class="wmnds-content-tile--container__wrapper">
      {{
      large({
      strongText: "Hagley Road",
      text: 'Usual traffic',
      iconLeft: "modes-isolated-roads",
      iconRight: 'general-success'
      })
      }}
   </div>
   <div class="wmnds-content-tile--container__wrapper">
      {{
      large({
      strongText: "Ring Road",
      text: 'Usual traffic',
      iconLeft: "modes-isolated-roads",

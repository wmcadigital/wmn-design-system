{% macro wmndsFeedbackLoop(params) %}

<div class="wmnds-feedback-loop">
  <div class="wmnds-container">
    <div class="wmnds-feedback-loop__useful">
      Is this page useful?

      <a href="#" title="link title" target="_self" class="wmnds-link wmnds-m-l-sm">Yes</a>
      <a href="#" title="link title" target="_self" class="wmnds-link wmnds-m-l-sm">No</a>
    </div>
    <div class="wmnds-feedback-loop__wrong">
      <a href="#" title="link title" target="_self" class="wmnds-link">Is there anything wrong with this page?</a>
    </div>
  </div>
</div>

{% endmacro %}
  "copyright": "WMCA",
  "sasslintConfig": ".sass-lint.yml",
  "browserslist": [
    "last 2 versions",
    "ie 8",
    "ie 9",
    "iOS 9"
  ],
  "buildDirs": {
    "live": {
      "baseUrl": "//beta.wmca.org.uk",
      "cdn": "http://cdn.wmca.org.uk/wmca/web"

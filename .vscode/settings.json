{
  "emmet.includeLanguages": {
    "nunjucks": "html",
    "njk": "html",
    "jinja-html": "html"
  },
  "html-css-class-completion.includeGlobPattern": "**/*.{css,html,njk, md}",
  "files.associations": {
    "*.njk": "jinja-html",
    "*.njk.md": "jinja-md"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "vsicons.associations.files": [
    {
      "icon": "nunjucks",
      "extensions": ["njk"],
      "format": "svg"
    }
  ],
  "files.eol": "\n"
}
